{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "schema_version",
    "ok",
    "command",
    "result"
  ],
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "ok": {
      "type": "boolean"
    },
    "command": {
      "const": "org.info"
    },
    "result": {
      "type": "object",
      "required": [
        "school_name",
        "logo_url",
        "source"
      ]
    }
  }
}
